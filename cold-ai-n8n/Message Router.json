      "parameters": {
        "jsCode": " // Message Generation Limits Validator\n  const result = $input.first().json;\n  const planType = result.plan_type || 'free';\n\n  // Initialize response\n  let canProceed = false;\n  let errorMessage = '';\n  let errorType = null;\n  let requiresLimitCheck = true;\n\n  // Get current usage\n  let messagesUsed = 0;\n  if (result.messages_generated !== undefined) {\n    messagesUsed = result.messages_generated;\n  } else if (result.usage_data && result.usage_data.length > 0) {\n    messagesUsed = result.usage_data[0].messages_generated || result.usage_data[0].messages_sent || 0;\n  }\n\n  // Define limits\n  const limits = {\n    free: { monthly_messages: 25 },\n    pro: { monthly_messages: 1000 },\n    enterprise: { monthly_messages: -1 }\n  };\n\n  const currentLimit = limits[planType]?.monthly_messages || limits.free.monthly_messages;\n\n  // Check limits - any request that reaches this point needs limit checking\n  if (currentLimit === -1) {\n    canProceed = true; // Unlimited\n  } else if (messagesUsed >= currentLimit) {\n    errorType = 'MONTHLY_LIMIT_EXCEEDED';\n    errorMessage = `You've reached your monthly message limit (${messagesUsed}/${currentLimit}). Upgrade your plan or wait for next month's reset.`;\n    canProceed = false;\n  } else {\n    canProceed = true;\n  }\n\n  // Build response\n  const response = {\n    ...result,\n    validation: {\n      can_proceed: canProceed,\n      requires_limit_check: requiresLimitCheck,\n      usage_info: {\n        messages_used: messagesUsed,\n        messages_limit: currentLimit,\n        messages_remaining: currentLimit === -1 ? 'unlimited' : Math.max(0, currentLimit - messagesUsed),\n        plan_type: planType\n      },\n      timestamp: new Date().toISOString()\n    }\n  };\n\n  // Add error details if cannot proceed\n  if (!canProceed) {\n    response.validation.error = {\n      type: errorType,\n      message: errorMessage,\n      suggestion: 'Upgrade your plan for more messages or wait for monthly reset'\n    };\n  }\n\n  return { json: response };\n"
      },
